{
    "name": "Streamline",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Streamline, a presentation productivity created for HackTheNorth++ 2020",
    "icons":{
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action":{
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://docs.google.com/presentation/*"],
            "js": ["content.js"],
            "all_frames": true
        }
    ],
    "permissions": ["tabs"],
    "options_page": "options.html"
}