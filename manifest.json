{
    "name": "Streamline",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Streamline, a presentation productivity created for HackTheNorth++ 2020",
    "icons":{
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action":{
        
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["tabs"],
    "options_page": "options.html"
}