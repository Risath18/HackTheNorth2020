{
    "name": "Streamline",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Streamline, a presentation productivity created for HackTheNorth++ 2020",
    "icons":{
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action":{
        "default_popup": "dashboard.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["tabs", "identity", "https://www.googleapis.com/*"],
    "options_page": "options.html",
    "oauth2": {
        "client_id": "965980921214-ummd5lmg4tt8g4ovqah08hjucrdc576r.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/presentations",
            "https://www.googleapis.com/auth/userinfo.email"
        ]
    }
}